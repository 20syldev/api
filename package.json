{
    "version": "3.0.0",
    "name": "@20syldev/api",
    "description": "Node.js API with multiple features. Check the documentation at https://docs.sylvain.pro",
    "main": "app.js",
    "type": "module",
    "scripts": {
        "start": "node app.js",
        "dev": "nodemon app.js",
        "build": "npm install && node app.js",
        "lint": "prettier --write .",
        "upgrade:minor": "npm upgrade",
        "upgrade:major": "npx npm-check-updates -u && npm install",
        "upgrade:build": "npm upgrade && npm install && node app.js"
    },
    "dependencies": {
        "canvas": "latest",
        "cors": "latest",
        "dotenv": "latest",
        "express": "latest",
        "mathjs": "latest",
        "node-fetch": "latest",
        "prettier": "latest",
        "qrcode": "latest",
        "random": "latest",
        "uuid": "latest"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/20syldev/api.git"
    },
    "keywords": [
        "api",
        "express",
        "utility",
        "math"
    ],
    "author": "Sylvain L.",
